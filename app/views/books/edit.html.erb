<h1>Editing Book</h1>

<%=form_with model: @book, url:update_book_path(@book.id), method: :patch do |f|%>

  <label>Title</label>
  <%=f.text_field :title%>

  <label>Body</label>
  <%=f.text_area :body%>

  <br>
  <%=f.submit'Create Box'%>

<%end%>

<%=link_to 'Show',book_path(@book.id)%>
<%=link_to 'Back',books_path(@book.id)%>